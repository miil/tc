;SGViewer (plugin for Total Commander) profile

[SGViewer Main]
DetectString=MULTIMEDIA & ext="BMP"|ext="JPG"|ext="JPE"|ext="JPEG"|ext="PNG"|ext="GIF"|ext="TIF"|ext="TIFF"|ext="PSD"|ext="TGA"|ext="PCX"|ext="PCD"|ext="PSP"|ext="WMF"|ext="EMF"

[SGViewer Setup]
WindowColor=0
StatusBarVisible=1
TimerInterval=5
ShowLoadPercentInMenu=0
SaveDialogFilterIndex=1
SaveJPEGProgressive=1
SaveNoJPEGRecompress=1
SaveJPEGQuality=80
FitOnlyBigImages=1
CenterImage=1
SortingType=2
ReverseSort=0
ResizeAspectRatio=1
ResizeResamplingFilter=5
ChangeListerCaption=0
ChangeImageAfterLoad=0
AllowFilesDelete=0
StatusBarPanelsAutoSize=1
AllowFileOperations=0
ShowWaitCursor=1
ShowAnimation=0
ShellApp0=
ShellApp1=
ShellApp2=
ShellAppCaption0=
ShellAppCaption1=
ShellAppCaption2=
ShowInfoLabel=1
DoubleBufferedDrawing=0
AutoHidesInQView=0
RandomViewMode=0
RunMode=0
HideCursorInFullScreen=0
LanguageFile=
EnableThumbnails=1
ToolbarLock=0
Antialiasing=0
UseIJL=0
InfoLabelFont=
AANotForBig=0,4000,1,4000,0,1
AntialiasingFilter=1
UseLibGFL=0
StartFitMode=0
ExtFileOptions=4
ThumbsOptions=0
FileSizeDisplay=3
CacheSettingsData=1
DontLoadInQuickView=0
WheelSkipCount=0

[SGViewer Setup_Toolbar]
ShowToolbarHints=1
Rev=2000
Visible=1
DockedTo=DockLeft
LastDock=DockLeft
DockRow=0
DockPos=0
FloatLeft=0
FloatTop=0
